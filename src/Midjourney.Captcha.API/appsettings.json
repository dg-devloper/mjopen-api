{
  "Demo": null, // Website configuration for demo mode
  "Captcha": {
    "Concurrent": 1, // Number of concurrent processes
    "Headless": true, // Run in headless mode
    "TwoCaptchaKey": "", // API Key for 2captcha.com
    "YesCaptchaKey": "" // API Key for yescaptcha.com
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": null,
          "rollOnFileSizeLimit": false,
          "retainedFileCountLimit": 31
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://*:8081"
}
